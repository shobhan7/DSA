import java.util.HashMap;


public class A  {



    public static void main(String[] args) {
        int array[]= {1, 2, 3, 4,1,2,3,4,5,6};
        HashMap<Integer,Integer> map=new HashMap<>();
        int n=array.length;
        for(int i:array){
            if(map.containsKey(i)){
                map.put(i,map.get(i)+1);

            }else{
                map.put(i,1);
            }

        }
        System.out.println(map);



    }

}

