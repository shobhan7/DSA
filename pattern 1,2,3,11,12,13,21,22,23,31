public class printingpattern {

    public static void main(String[] args) {

        int n=33;
        printnum(n);
    }

    private static void printnum(int n) {
        int c=0;
        for (int i=1;i<=n*n;i++) {
            if((i%10==1||i%10==2||i%10==3)&&(c!=n)) {
                System.out.println(i);
                c++;
            }

        }

    }
}
