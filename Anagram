import java.util.Arrays;
public class anagram {
public static void main(String[]args){

    String a = "dsa";
    String b = "ads";

    anagramstring(a, b);

}
   public static void anagramstring(String a, String b){
    char[] chars = a.toCharArray();
    Arrays.sort(chars);
    char[] chars2 = b.toCharArray();
    Arrays.sort(chars2);
       boolean result= Arrays.equals(chars,chars2);
       if(result){
           System.out.println("true its anagram");
       }else {
           System.out.println("false not anagram");
       }


   }

}
